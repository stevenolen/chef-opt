# Managed by Chef for node <%= node['fqdn'] -%>.

omniauth:
  - :keycloak_openid
  - opt
  - client_options:
      site: <%= @config.keycloak_site %>
      realm: shibboleth
    authorize_params:
      kc_idp_hint: uclashib

esb_secret:
  user: <%= @config.esb_user %>
  pass: <%= @config.esb_pass %>
  auth_url: <%= @config.esb_auth_url %>
  cert: <%= @config.esb_cert %>
  key: <%= @config.esb_key %>
