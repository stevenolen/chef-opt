# Managed by Chef for node <%= node['fqdn'] -%>.

oauth2_provider_shibboleth:
  key: <%= @config.shib_client_name %>
  secret: <%= @config.shib_secret %>
  properties:
    site: <%= @config.shib_site %>
    authorize_url: /oauth2/authorize
    token_url: /oauth2/access_token
  routes:
    get_user: /oauth2/user
    
esb_secret:
  user: <%= @config.esb_user %>
  pass: <%= @config.esb_pass %>
  auth_url: <%= @config.esb_auth_url %>
  cert: <%= @config.esb_cert %>
  key: <%= @config.esb_key %>