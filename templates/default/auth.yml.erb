# Managed by Chef for node <%= node['fqdn'] -%>.

omniauth:
  - :keycloak_openid
  - opt
  - client_options:
      site: <%= @config.keycloak_site %>
      realm: shibboleth
    authorize_params:
      kc_idp_hint: uclashib

registrar:
  auth_url: <%= @config.registrar_auth_url %>
  url_base: <%= @config.registrar_url_base %>
  key: <%= @config.registrar_key %>
  secret: <%= @config.registrar_secret %>
